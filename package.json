{
    "name": "demo-payments",
    "version": "1.0.0",
    "description": "",
    "main": "src/main/App.js",
    "keywords": [
        "blockchain",
        "ethereum",
        "hardhat",
        "smart-contracts",
        "solidity"
    ],
    "author": "",
    "license": "ISC",
    "scripts": {
        "start": "MY_ENV=.production LOG_LEVEL=debug DD_ENV=prod DD_LOGS_INJECTION=true DD_RUNTIME_METRICS_ENABLED=true node --require dd-trace/init src/main/App.js",
        "startdocker": "MY_ENV= LOG_LEVEL=debug node src/main/App.js",
        "startdev": "MY_ENV=.development LOG_LEVEL=debug nodemon src/main/App.js",
        "test": "MY_ENV=.test LOG_LEVEL=debug nyc --reporter=lcov --reporter=text mocha --exit",
        "compile": "MY_ENV=.development hardhat compile",
        "prepare": "husky install",
        "postinstall": "npm run compile",
        "clean": "hardhat clean",
        "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
        "size-contracts": "hardhat size-contracts",
        "docgen": "hardhat docgen",
        "deploy-kovan": "hardhat deploy --network kovan",
        "deploy-local": "hardhat deploy --network hardhat",
        "release": "standard-version"
    },
    "dependencies": {
        "axios": "0.21.1",
        "bcrypt": "5.0.1",
        "body-parser": "1.19.0",
        "cors": "2.8.5",
        "crypto-js": "4.0.0",
        "dd-trace": "2.3.1",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "firebase": "9.6.7",
        "fs-extra": "10.0.1",
        "hardhat": "2.0.6",
        "hardhat-deploy": "0.7.4",
        "hardhat-deploy-ethers": "0.3.0-beta.7",
        "node-html-parser": "4.1.0",
        "nodemailer": "6.6.3",
        "nodemailer-sendgrid-transport": "0.2.0",
        "nodemon": "2.0.15",
        "nyc": "15.1.0",
        "pg": "8.7.3",
        "pg-hstore": "2.3.4",
        "rewire": "6.0.0",
        "sequelize": "6.6.5",
        "sequelize-mock": "0.10.2",
        "simple-node-logger": "21.8.12",
        "sinon": "13.0.1",
        "solc": "0.7.4",
        "sqlite3": "5.0.2",
        "swagger-jsdoc": "5.0.0",
        "swagger-ui-express": "4.3.0",
        "truffle-hdwallet-provider": "1.0.17",
        "ws": "7.5.2"
    },
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 2017
        },
        "env": {
            "es6": true
        }
    },
    "devDependencies": {
        "@commitlint/cli": "9.1.2",
        "@commitlint/config-conventional": "12.0.1",
        "@ethersproject/abstract-signer": "5.0.6",
        "@ethersproject/bignumber": "5.0.8",
        "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.7",
        "@nomiclabs/hardhat-solhint": "2.0.0",
        "@nomiclabs/hardhat-waffle": "2.0.0",
        "@openzeppelin/contracts": "3.4.0",
        "@typechain/ethers-v5": "5.0.0",
        "@types/chai": "4.2.13",
        "@types/fs-extra": "9.0.1",
        "@types/mocha": "8.2.0",
        "@types/node": "14.14.25",
        "@types/rewire": "^2.5.28",
        "@typescript-eslint/eslint-plugin": "4.14.2",
        "@typescript-eslint/parser": "4.14.2",
        "bignumber.js": "9.0.1",
        "chai": "4.3.6",
        "chai-http": "4.3.0",
        "dotenv": "8.2.0",
        "eslint": "7.11.0",
        "eslint-config-prettier": "6.12.0",
        "eth-revert-reason": "^1.0.3",
        "ethereum-waffle": "3.2.0",
        "ethers": "^5.6.9",
        "expect": "27.0.6",
        "fastify": "3.14.2",
        "fetch-mock": "9.11.0",
        "hardhat-contract-sizer": "2.0.2",
        "hardhat-docgen": "^1.3.0",
        "hardhat-gas-reporter": "1.0.4",
        "hardhat-preprocessor": "0.1.2",
        "hardhat-prettier": "0.0.1",
        "hardhat-typechain": "0.3.4",
        "husky": "5.0.9",
        "jest": "27.5.1",
        "jest-when": "3.5.1",
        "leafleth": "0.0.9",
        "mocha": "9.2.2",
        "prettier": "2.2.1",
        "prettier-plugin-solidity": "1.0.0-beta.5",
        "shelljs": "^0.8.5",
        "solhint": "3.3.2",
        "solhint-plugin-prettier": "0.0.5",
        "solidity-coverage": "^0.7.21",
        "standard-version": "^9.5.0",
        "ts-generator": "0.1.1",
        "ts-node": "9.1.1",
        "typechain": "4.0.1",
        "typescript": "4.1.3"
    },
    "nyc": {
        "exclude": [
            "src/main/App.js",
            "src/main/SwaggerConfig.js",
            "src/others/constants.js",
            "src/services/Logger.js",
            "**/*.test.js",
            "**/*.test.ts",
            "test/mocks/*",
            "coverage/*"
        ]
    },
    "files": [
        "/contracts"
    ],
    "types": "types/index.d.ts"
}
